# greatest among 4 no.s
a=int(input("Enter 1st no.: "))
b=int(input("Enter 2nd no.: "))
c=int(input("Enter 3rd no.: "))
d=int(input("Enter 4th no.: "))

if a>b and a>c and c>d:
    print(a,"is greatest")
elif b>a and b>c and c>d:
    print(b,"is greatest")
elif c>a and c>b and b>d:
    print(c,"is greatest")
else:
    print(d,"is greatest") 
